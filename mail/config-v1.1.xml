<?xml version="1.0" encoding="UTF-8"?>
<clientConfig version="1.1">
	<!-- locate at http://autoconfig.mylansite.org/mail/config-v1.1.xml -->
	<emailProvider id="Penguins.ml">
		<domain>Penguins.ml</domain>
		<displayName>Penguins.ml Email Domain</displayName>
		<displayShortName>Penguins.ml</displayShortName>
		<incomingServer type="imap">
			<hostname>friday.mxroute.com</hostname>
			<port>993</port>
			<socketType>SSL</socketType>
			<authentication>password-encrypted</authentication>
			<username>%EMAILADDRESS%</username>
		</incomingServer>
        <incomingServer type="pop3">
			<hostname>friday.mxlogin.com</hostname>
			<port>995</port>
			<socketType>SSL</socketType>
			<authentication>password-encrypted</authentication>
			<username>%EMAILADDRESS%</username>
		</incomingServer>
		<outgoingServer type="smtp">
			<hostname>friday.mxroute.com</hostname>
			<port>465</port>
			<socketType>SSL</socketType>
			<authentication>password-encrypted</authentication>
			<username>%EMAILADDRESS%</username>
		</outgoingServer>
	</emailProvider>
</clientConfig>
